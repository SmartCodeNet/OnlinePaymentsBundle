{
  "name": "GG/online-payments-bundle",
  "description": "Symfony bundle online payments",
  "type": "symfony-bundle",
  "keywords": ["online payments", "php", "symfony", "bundle",
    "PayU", "Blue Media"],
  "license": "MIT",
  "minimum-stability": "dev",
  "authors": [
    {
      "name": "Krzysztof Orzo≈Ç",
      "email": "krzysztoforzol@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-soap": "*",
    "ext-simplexml": "*",
    "symfony/http-client": "5.0.*"
  },
  "require-dev": {
  },
  "config": {
    "preferred-install": {
      "*": "dist"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "GG\\OnlinePaymentsBundle\\": ""
    },
    "exclude-from-classmap": [
      "/Tests/"
    ]
  }
}
